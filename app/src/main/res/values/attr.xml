<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTitleBar">
        <attr name="leftText" format="string"/>
        <attr name="leftTextColor" format="color"/>
        <attr name="leftTextSize" format="integer"/>
        <attr name="centerTextColor" format="color"/>
        <attr name="centerTitle" format="string"/>
        <attr name="centerTitleSize" format="integer"/>
        <attr name="rightTextColor" format="color"/>
        <attr name="rightText" format="string"/>
        <attr name="rightTextSize" format="integer"/>
        <attr name="leftDrawable" format="reference"/>
        <attr name="rightDrawable" format="reference"/>
    </declare-styleable>
</resources>
